.template on
IF OBJECT_ID('dbo.$(../@name:u_)_SelectBy_$(@name:u_)') IS NULL
	EXEC('CREATE PROC dbo.$(../@name:u_)_SelectBy_$(@name:u_) AS RETURN 1')
GO

ALTER PROC dbo.$(../@name:u_)_SelectBy_$(@name:u_)
.foreach ../field[@pk='true']
	@$(@name:u_) $(@db-type??@type)$(",":~)
.endfor
AS
SELECT 
.foreach ../field
	[$(@name:u_)]$(",":~)
.endfor
FROM dbo.$(../@name:u_)
WHERE 
	[$(@name:u_)] = @$(@name:u_)
GO