.template on
IF OBJECT_ID('dbo.$(../@name:u_)_DeleteBy_$(@name:u_)') IS NULL
	EXEC('CREATE PROC dbo.$(../@name:u_)_DeleteBy_$(@name:u_) AS RETURN 1')
GO

ALTER PROC dbo.$(../@name:u_)_DeleteBy_$(@name:u_)
	@$(@name:u_) $(@db-type??@type)
AS
DELETE FROM dbo.$(../@name:u_)
WHERE 
	[$(@name:u_)] = @$(@name:u_)
GO