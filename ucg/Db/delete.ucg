.template on
IF OBJECT_ID('dbo.$(@name:u_)_DELETE') IS NULL
	EXEC('CREATE PROC dbo.$(@name:u_)_DELETE AS RETURN 1')
GO

ALTER PROC dbo.$(@name:u_)_DELETE
.foreach field[@pk]
	@$(@name:u_) $(@db-type??@type)$(",":~)
.endfor
AS
DELETE FROM dbo.$(@name:u_)
WHERE 
.foreach field[@pk]
	[$(@name:u_)] = @$(@name:u_)$(" AND":~)
.endfor
GO