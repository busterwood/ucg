.template on
@page
@model $(../@cs-namespace).$(@name:p)Pages.IndexModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<table>
	<thead>
		<tr>
.foreach field
			<th>$(@name)</th>
.endfor
		</tr>	
	</thead>
	<tbody>
@foreach(var item in Model.$(@name:p)s)
{
		<tr>
.foreach field
.  if string(@fk)
			<td> <a asp-page="../$(@fk:p)/Detail" asp-route-id="@item.$(@name:p)">@item.$(@name:p)</a> </td>
.  else
			<td>@item.$(@name:p)</td>
.  endif
.endfor
		</tr>
}
	</tbody>
</table>