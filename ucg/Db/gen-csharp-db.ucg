.template off
// merge domain types with fields so each field has full type attributes
foreach entity
	foreach field
		mergexml //type[@typename='$(@typename)']
	endfor
endfor

foreach entity
	output "$(@name:p).cs"	
	include "cs-class.ucg"
	
	output "$(@name:p)Repository.cs"	
	include "cs-repository.ucg"
endfor