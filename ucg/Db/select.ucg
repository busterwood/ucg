.template on
IF OBJECT_ID('dbo.$(@name:u_)_Select') IS NULL
	EXEC('CREATE PROC dbo.$(@name:u_)_Select AS RETURN 1')
GO

ALTER PROC dbo.$(@name:u_)_Select
.foreach field[@pk]
	@$(@name:u_) $(@db-type??@type)$(",":~)
.endfor
AS
SELECT 
.foreach field
	[$(@name:u_)]$(",":~)
.endfor
FROM dbo.$(@name:u_)
WHERE 
.foreach field[@pk]
	[$(@name:u_)] = @$(@name:u_)$(" AND":~)
.endfor
GO