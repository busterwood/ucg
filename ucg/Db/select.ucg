.template on
IF OBJECT_ID('dbo.$(@name:sql)_Select') IS NULL
	EXEC('CREATE PROC dbo.$(@name:sql)_Select AS RETURN 1')
GO

ALTER PROC dbo.$(@name:sql)_Select
.foreach field[@pk='true']
	@$(@name:sql) $(@type)$(",")
.end
AS
SELECT 
.foreach field
	[$(@name:sql)]$(",")
.end
FROM dbo.$(@name:sql)
WHERE 
.foreach field[@pk='true']
	[$(@name:sql)] = @$(@name:sql)$(" AND")
.end
GO