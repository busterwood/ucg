.template on
IF OBJECT_ID('dbo.$(@name:sql)_Insert') IS NULL
	EXEC('CREATE PROC dbo.$(@name:sql)_Insert AS RETURN 1')
GO

ALTER PROC dbo.$(@name:sql)_Insert
.foreach field
	@$(@name:sql) $(@db-type??@type)$(",")
.end
AS
INSERT INTO dbo.$(@name:sql) (
.foreach field
	[$(@name:sql)]$(",")
.end
) VALUES (
.foreach field
	@$(@name:sql)$(",")
.end
)
GO