<?xml version="1.0" encoding="utf-8" ?>
<entities cs-namespace="BusterWood.Testing">
  <entity name="User">
    <field name="User Id" nulls="not null" type="int" pk="true"/>
    <field name="Full Name" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
    <field name="Email" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
  </entity>
  
  <entity name="Fund">
    <field name="Fund Id" nulls="not null" type="int" pk="true"/>
    <field  name="Name" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
  </entity>
  
  <entity name="Book">
    <field name="Book Id" nulls="not null" type="int" pk="true"/>
    <field name="Fund Id" nulls="not null" type="int" fk="Fund"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
  </entity>
  
  <entity name="Strategy">
    <field name="Strategy Id" nulls="not null" type="int" pk="true"/>
    <field  name="Name" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
  </entity>
  
  <entity name="Security">
    <field name="Security Id" nulls="not null" type="int" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="50"/>
    <field name="BB Ticker" nulls="not null" type="string" db-type="VarChar" db-size="10" cs-size=",10"/>
    <field name="Security Class Id" nulls="not null" type="byte" db-type="TinyInt" fk="Security Class"/>  
  </entity>

  <entity name="Investment">
    <field name="Investment Id" nulls="not null" type="int" pk="true"/>
    <field name="Decision Maker" nulls="not null" type="int" fk="User" fk-field="User Id"/>
    <field name="Decision Date" nulls="not null" type="DateTime"/>
    <field name="Inputter" nulls="not null" type="int" fk="User" fk-field="User Id"/>
  </entity>
  
  <entity name="Order">
    <field name="Order Id" nulls="not null" type="int" pk="true"/>
    <field name="Investment Id" nulls="not null" type="int" fk="Investment" fk-rel="owner"/>
    <field name="Security Id" nulls="not null" type="int" fk="Security"/>
    <field name="Limit Price" nulls="null" type="decimal"/>  
    <field name="Order Price Type Id" nulls="not null" type="byte" db-type="tinyint" fk="Order Price Type"/>  
    <field name="Investment Type Id" nulls="not null" type="byte" db-type="tinyint" fk="Investment Type"/>  
    <field name="Market Type Id" nulls="not null" type="byte" db-type="tinyint" fk="Market Type"/>  
    <field name="Currency Code" nulls="not null" type="string" db-type="char" db-size="3" fk="Currency"/>  
  </entity>
  
  <entity name="Order Line">
    <field name="Order Line Id" nulls="not null" type="int" pk="true"/>
    <field name="Order Id" nulls="not null" type="int" fk="Order" fk-rel="owner"/>
    <field name="Fund Id" nulls="not null" type="int" fk="Fund"/>
    <field name="Book Id" nulls="not null" type="int" fk="Book"/>
    <field name="Strategy Id" nulls="not null" type="int" fk="Strategy"/>
    <field name="Quantity" nulls="not null" type="int"/>
  </entity>

  <entity name="Currency">
    <field name="Currency Code" nulls="not null" type="string" db-type="char" db-size="3" pk="true"/>
    <field name="Currency Id" nulls="not null" type="short" db-type="SmallInt" uk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="30"/>
    <static>
      <data CurrencyCode="AUD" CurrencyId="36" Name="Australian dollar"/>
      <data CurrencyCode="CAD" CurrencyId="124" Name="Canadian dollar"/>
      <data CurrencyCode="CHF" CurrencyId="756" Name="Swiss franc"/>
      <data CurrencyCode="CNY" CurrencyId="156" Name="Renminbi (Chinese) yuan"/>
      <data CurrencyCode="DKK" CurrencyId="208" Name="Danish krone"/>
      <data CurrencyCode="GBP" CurrencyId="826" Name="Pound sterling"/>
      <data CurrencyCode="HKD" CurrencyId="344" Name="Hong Kong dollar"/>
      <data CurrencyCode="INR" CurrencyId="356" Name="Indian rupee"/>
      <data CurrencyCode="JPY" CurrencyId="392" Name="Japanese yen"/>
      <data CurrencyCode="KRW" CurrencyId="410" Name="South Korean won"/>
      <data CurrencyCode="MXN" CurrencyId="484" Name="Mexican peso"/>
      <data CurrencyCode="NOK" CurrencyId="578" Name="Norwegian krone"/>
      <data CurrencyCode="NZD" CurrencyId="554" Name="New Zealand dollar"/>
      <data CurrencyCode="SEK" CurrencyId="752" Name="Swedish krona"/>
      <data CurrencyCode="USD" CurrencyId="840" Name="United States dollar"/>
    </static>
  </entity>

  <entity name="Broker Capacity">
    <field name="Capacity Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data CapacityId="1" Name="Agent"/>
      <data CapacityId="2" Name="Principle"/>
    </static>
  </entity>

  <entity name="Commission Type">
    <field name="Commission Type Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data CommissionTypeId="1" Name="Per share"/>
      <data CommissionTypeId="2" Name="Basis points"/>
      <data CommissionTypeId="3" Name="Percentage"/>
      <data CommissionTypeId="4" Name="Fixed"/>
    </static>
  </entity>

  <entity name="Fill Status">
    <field name="Fill Status Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data FillStatusId="1" Name="Cancelled"/>
      <data FillStatusId="1" Name="Done for day"/>
      <data FillStatusId="1" Name="Expired"/>
      <data FillStatusId="1" Name="Filled"/>
      <data FillStatusId="1" Name="New"/>
      <data FillStatusId="1" Name="Partially filled"/>
      <data FillStatusId="1" Name="Pending cancel"/>
      <data FillStatusId="1" Name="Pending replace"/>
      <data FillStatusId="1" Name="Rejected"/>
