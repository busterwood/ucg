  </entity>

  <entity name="Execution Medium">
    <field name="Execution Medium Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data ExecutionMediumId="1" Name="Electronic"/>
      <data ExecutionMediumId="1" Name="Paper"/>
    </static>  </entity>

  <entity name="Order Price Type">
    <field name="Order Price Type Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data OrderPriceTypeId="1" Name="Market"/>
      <data OrderPriceTypeId="1" Name="Limit"/>
    </static>
  </entity>

  <entity name="Fill Booking Type">
    <field name="Fill Booking Type Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
    <static>
      <data FillBookingTypeId="1" Name="Average price"/>
      <data FillBookingTypeId="1" Name="By Price"/>
    </static>
  </entity>

  <entity name="Instrument Class">
    <field name="Instrument Class Id" nulls="not null" type="byte" db-type="TinyInt" pk="true"/>
    <field name="Name" nulls="not null" type="string" db-type="VarChar" db-size="20"/>
