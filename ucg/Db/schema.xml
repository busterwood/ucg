<?xml version="1.0" encoding="utf-8" ?>
<entities>
  <entity name="User">
    <field name="User Id" type="int" pk="true"/>
    <field name="Full Name" type="string"/>
    <field name="Email" type="string"/>
  </entity>
  <entity name="Fund">
    <field name="Fund Id" type="int" pk="true"/>
    <field  name="Name" type="string"/>
  </entity>
  <entity name="Book">
    <field name="Book Id" type="int" pk="true"/>
    <field name="Name" type="string"/>
  </entity>
  <entity name="Strategy">
    <field name="Strategy Id" type="int" pk="true"/>
    <field  name="Name" type="string"/>
  </entity>
  <entity name="Security">
    <field name="Security Id" type="int" pk="true"/>
    <field name="Name" type="string"/>
    <field name="BB Ticker" type="string"/>
  </entity>

  <entity name="Investment">
    <field name="Investment Id" type="int" pk="true"/>
    <field name="Decision Maker" type="int" fk="true"/>
    <field name="Decision Date" type="datetime"/>
    <field name="Inputter" type="int" fk="true"/>
  </entity>
  
  <entity name="Order">
    <field name="Order Id" type="int" pk="true"/>
    <field name="Investment Id" type="int" fk="true"/>
    <field name="Security Id" type="int" fk="true"/>
  </entity>
  
  <entity name="Order Line">
    <field name="Order Line Id" type="int" pk="true"/>
    <field name="Order Id" type="int" fk="true"/>
    <field name="Fund Id" type="int" fk="true"/>
    <field name="Book Id" type="int" fk="true"/>
    <field name="Strategy Id" type="int" fk="true"/>
    <field name="Quantity" type="int"/>
  </entity>
</entities>
